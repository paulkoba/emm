fn weird_fn(x: i64) -> i64 {
    let y = x + 1
    let z = x + 2
    return y + z
}

fn very_weird_fn(x : i32) -> i64 {
    let y: i64 = x as i64 + 5
    let z: i32 = x + 10i32
    return y + (z as i64)
}

fn iterative_factorial(x: i64) -> i64 {
    let result = 1
    let idx = 1

    while idx <= x {
        result = result * idx
        idx = idx + 1
    }

    return result
}

fn main() -> i64 {
    return iterative_factorial(5)
}