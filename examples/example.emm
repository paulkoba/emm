fn factorial(n: i32) -> i32 {
    if (n == 0) {
        return 1;
    } else {
        # Comment example #1
        return n * factorial(n - 1);
    }
}

# Comment example #2
fn main() -> i32 {
    let a: i64 = 123; # Comment example #3
    let b: i64 = 456;

    let c = a + b;

    let mut d = 42;
    d = factorial(d + 1) - c;

    return d;
}