extern fn print_i64(n : i64) -> i64
extern fn print_ptr(n : Pointer<i64>) -> Pointer<i64>

fn dereference_something(n : i64) -> i64 {
    print_i64(n)

    let ptr : Pointer<i64> = &n

    print_ptr(ptr)
    print_i64(*ptr)

    return 0
}

fn main() -> i64 {
    dereference_something(5)

    return 0
}